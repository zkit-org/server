<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zkit.support.server.account.auth.mapper.AuthAccountRoleMapper">

    <resultMap id="ResultMap" type="org.zkit.support.server.account.auth.entity.dto.AuthAccountRole" extends="org.zkit.support.server.account.auth.mapper.base.AuthAccountRoleBaseMapper.BaseResultMap">
    </resultMap>

    <sql id="Column_List">
        <include refid="org.zkit.support.server.account.auth.mapper.base.AuthAccountRoleBaseMapper.Base_Column_List"/>
    </sql>
    <delete id="deleteByAccountId">
        delete
        from auth_account_role
        where account_id = #{accountId,jdbcType=NUMERIC}
    </delete>
    <delete id="deleteByRoleId">
        delete
        from auth_account_role
        where role_id = #{roleId,jdbcType=NUMERIC}
    </delete>

</mapper>
