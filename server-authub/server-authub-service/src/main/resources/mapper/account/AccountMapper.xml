<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zkit.support.server.authub.account.mapper.AccountMapper">

    <resultMap id="ResultMap" type="org.zkit.support.server.authub.account.entity.dto.Account" extends="org.zkit.support.server.authub.account.mapper.base.AccountBaseMapper.BaseResultMap">
    </resultMap>

    <sql id="Column_List">
        <include refid="org.zkit.support.server.authub.account.mapper.base.AccountBaseMapper.Base_Column_List"/>
    </sql>

    <select id="selectAll" resultMap="ResultMap">
        select
        <include refid="Column_List"/>
        from account
        where deleted = false
        order by create_time desc
    </select>
</mapper>
